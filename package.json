{
    "name": "vuestarter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "npm run serve",
        "serve": "vue-cli-service serve",
        "server": "vue-cli-service build && http-server ./dist",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "lint-autofix": "eslint --ext .js,.vue src  --fix",
        "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
        "precommit": "npm run eslint-check && npm run lint",
        "deploy": "gh-pages-deploy",
        "clean-source": "rimraf README.md src webroot package.json"
    },
    "dependencies": {
        "babel-plugin-transform-imports": "^1.5.1",
        "babel-polyfill": "^6.26.0",
        "lodash": "^4.17.11",
        "path": "^0.12.7",
        "poststylus": "^1.0.0",
        "unirest": "^0.6.0",
        "vue": "^2.5.21",
        "vue-router": "^3.0.2",
        "vuetify": "^1.3.0",
        "vuex": "^3.0.1",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@vue/cli-plugin-babel": "^3.3.0",
        "@vue/cli-plugin-eslint": "^3.3.0",
        "@vue/cli-service": "^3.3.0",
        "babel-eslint": "^10.0.1",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.0",
        "eslint": "^5.3.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.5.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.11.0",
        "eslint-plugin-vue": "^5.1.0",
        "gh-pages-deploy": "^0.5.1",
        "node-sass": "^4.11.0",
        "node-sass-magic-importer": "^5.3.0",
        "prettier": "^1.15.3",
        "rucksack-css": "^1.0.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue-cli-plugin-vuetify": "^0.4.6",
        "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
        "vue-loader": "^15.5.1",
        "vue-template-compiler": "^2.5.21",
        "vuetify-loader": "^1.1.1"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "gh-pages-deploy": {
        "staticpath": "dist",
        "noprompt": false
    }
}
